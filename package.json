{
  "name": "titanium-backbone",
  "version": "0.0.12",
  "description": "Framework for Titanium apps built with Backbone",
  "private": true,
  "dependencies": {
    "async": "0.1.x",
    "backbone": "0.9.x",
    "commander": "0.5.x",
    "jade": "0.20.x",
    "pretty-data": "0.20.x",
    "underscore": "1.3.x",
    "node-uuid": "1.3.x",
    "stitch-up":"git+ssh://git@github.com:trabian/stitch-up.git#master"
  },
  "bin": {
    "titanium-backbone": "./bin/titanium-backbone"
  },
  "main": "./lib/index.coffee",
  "stitch": {
    "paths": [
      "src"
    ],
    "dependencies": [
      "node_modules/underscore/underscore.js",
      "node_modules/backbone/backbone.js",
      "deps/console.js"
    ]
